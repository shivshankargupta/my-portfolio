<div class="d-flex list-unstyled  " style="margin-left: 150px;">
  <li class="font-size:50px;"><a class="text-white" href="/">Home/</a></li>
  <li class="ml-2 "><a class="text-white" href="#">projects</a></li>
</div>

<div class="container">
  <!-- Projects Page-->
  <h1>{{title}}</h1>
  <div class="row no-padding">
    {{#each projects}}
    <div class="col-sm-4 mt-4 mb-4">
      <div class="card">
        <a href="/projects/{{_id}}"><img class="card-img-top" src="{{image}}" alt="" height="250px" /></a>
        <div class="card-body">
          <h4 class="card-title">
            <a href="/projects/{{_id}}">{{this.title}}</a>
          </h4>
          <p class="card-text">
            {{this.description}}
          </p>
          <div class="btn-group">
            <button type="button" class="btn btn-sm btn-outline-secondary"><a 
                href="https://github.com/">Details</a></button>
            <button type="button" class="btn btn-sm btn-outline-secondary"><a 
                href="https://github.com/ bg-white" >Github</a></button>
          </div>

        </div>
      </div>
    </div>
    {{/each}}
  </div>
</div>




